<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BadgeImageView">
        <attr name="showText" format="boolean" />
        <attr name="labelPosition" format="enum">
            <enum name="leftTop" value="0" />
            <enum name="rightTop" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="OneLineCell">
        <attr name="oneLineImageResource" format="integer" />
        <attr name="isSwitchSetting" format="boolean" />
        <attr name="isTextSetting" format="boolean" />
        <attr name="oneLineMainText" format="string" />
        <attr name="oneLineSettingText" format="string" />
    </declare-styleable>

    <declare-styleable name="TwoLineCell">
        <attr name="twoLineImageResource" format="integer" />
        <attr name="twoLineMainText" format="string" />
        <attr name="twoLineSubText" format="string" />
    </declare-styleable>

</resources>