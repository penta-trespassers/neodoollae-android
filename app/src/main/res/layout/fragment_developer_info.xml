<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.login.main.mypage.settings.NotificationSettingsFragment" >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/bg_common_card">

        <com.pentatrespassers.neodoollae.common.view.TwoLineCell
            android:id="@+id/teamCell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:twoLineMainText="@string/team_name"
            app:twoLineSubText="@string/team_description"
            app:twoLineImageResource="@drawable/ic_mypage_setting_developers"/>


        <View
            android:id="@+id/divider1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/teamCell"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="72dp"
            android:layout_marginEnd="16dp"
            android:background="@color/color_card_divider"/>

        <com.pentatrespassers.neodoollae.common.view.TwoLineCell
            android:id="@+id/firstMemberCell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_common_button_menu_cell"
            app:layout_constraintTop_toBottomOf="@id/divider1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:twoLineMainText="@string/first_member"
            app:twoLineSubText="@string/back_end"
            app:twoLineImageResource="@drawable/ic_common_account_no_padding"/>

        <View
            android:id="@+id/divider2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/firstMemberCell"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="72dp"
            android:layout_marginEnd="16dp"
            android:background="@color/color_card_divider"/>

        <com.pentatrespassers.neodoollae.common.view.TwoLineCell
            android:id="@+id/secondMemberCell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/divider2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:twoLineMainText="@string/second_member"
            app:twoLineSubText="@string/front_end_designer"
            app:twoLineImageResource="@drawable/ic_common_account_no_padding"/>

        <View
            android:id="@+id/divider3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/secondMemberCell"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="72dp"
            android:layout_marginEnd="16dp"
            android:background="@color/color_card_divider"/>

        <com.pentatrespassers.neodoollae.common.view.TwoLineCell
            android:id="@+id/thirdMemberCell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/divider3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:twoLineMainText="@string/third_member"
            app:twoLineSubText="@string/front_end"
            app:twoLineImageResource="@drawable/ic_common_account_no_padding"/>

        <View
            android:id="@+id/divider4"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/thirdMemberCell"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="72dp"
            android:layout_marginEnd="16dp"
            android:background="@color/color_card_divider"/>

        <com.pentatrespassers.neodoollae.common.view.TwoLineCell
            android:id="@+id/fourthMemberCell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_common_button_menu_cell"
            app:layout_constraintTop_toBottomOf="@id/divider4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:twoLineMainText="@string/fourth_member"
            app:twoLineSubText="@string/front_back_end"
            app:twoLineImageResource="@drawable/ic_common_account_no_padding"/>

        <View
            android:id="@+id/divider5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/fourthMemberCell"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="72dp"
            android:layout_marginEnd="16dp"
            android:background="@color/color_card_divider"/>

        <com.pentatrespassers.neodoollae.common.view.TwoLineCell
            android:id="@+id/fifthMemberCell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/divider5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:twoLineMainText="@string/fifth_member"
            app:twoLineSubText="@string/front_end"
            app:twoLineImageResource="@drawable/ic_common_account_no_padding"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>